{
  "name": "ludus-flashcards",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "csv-parser": "^3.2.0",
    "firebase": "^11.9.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "./start-server.sh",
    "start:dev": "react-scripts start",
    "start:safari": "HOST=127.0.0.1 WDS_SOCKET_HOST=127.0.0.1 react-scripts start",
    "start:host": "HOST=localhost react-scripts start",
